{
  "name": "my-spork",
  "version": "1.0.0",
  "description": "Adv packaging system",
  "main": "index.js",
  "scripts": {
    "presmoosh": "uglifyjs ./lib/* --compress --mangle --output ./dist/lib.js --source-map",
    "smoosh": "uglifyjs ./src/* --compress --mangle --output ./dist/index.js --source-map",
    "build": "node terp-it",
    "test": "cd dist && browser-sync start --server --files \"*.js\"",
    "pretest": "npm run build"
  },
