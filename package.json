  "license": "ISC",
  "devDependencies": {
    "async": "^3.1.0",
    "browsersync": "0.0.1-security",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {}
}
